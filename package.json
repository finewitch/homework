{
  "name": "node-starter-es6",
  "version": "1.0.0",
  "description": "NodeJS Starter Repository With ES6",
  "main": "server.js",
  "scripts": {
    "test": "jest",
    "build": "babel --extensions '.ts' src/index.ts src/utils.ts --out-dir dist",
    "dev": "babel-watch server.js",
    "prelint:js": "npm run format:all",
    "lint:js": "eslint ./*.js",
    "format:all": "prettier --write *.js"
  },
  "keywords": [
    "NodeJS",
    "Express",
    "ES6",
    "Starter Repository"
  ],
  "author": "Burhan Bharmal",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.15",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-watch": "^2.0.8",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^3.4.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^26.6.3",
    "prettier": "^1.15.3",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  }
}
